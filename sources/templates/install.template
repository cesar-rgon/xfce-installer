<h2>{{prerequisites}}</h2>
<ul>
	{{#each prerequisiteList}}
		<li>{{{prerequisite}}}.</li>
	{{/each}}
</ul><br />

<h2>{{installation}}</h2>
<div class="installation">
	<label>{{installationDescription}}</label>
	<pre><code>$ wget https://github.com/cesar-rgon/xfce-installer/archive/master.tar.gz
$ tar -xvf master.tar.gz
$ cd xfce-installer-master
$ sudo ./install.sh</code></pre>
	
	{{#each imageList}}
		<img src={{source}} alt={{description}}/>
	{{/each}}
</div>
